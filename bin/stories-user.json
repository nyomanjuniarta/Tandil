        "properties": {
            "clue": {
                "title": "Look at the three contiguous buildings",
                "image": {
                    "url": "https://assets.pilot4.crosscult.uop.gr/users/5a39997761414b0001f14b2d/clue.png",
                    "copyright": null,
                    "type": null,
